<Window x:Class="DatabaseSwitcher.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DatabaseSwitcher"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Button Grid.Column="0" Name="uxSwitchToRmsBtn" Click="UxSwitchToRmsBtn_OnClick">Switch to RMS</Button>
        <Button Grid.Column="1" Name="uxSwitchToPtsBtn" Click="uxSwitchToPtsBtn_Click">Switch to PTS</Button>
    </Grid>
</Window>
